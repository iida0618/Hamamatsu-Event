    <form id="form-area" class="form-area">
    <div id="f1" class="f1">
        <h3 class="form-item">イベント情報</h3>
        <div class="detail-form">
        <label for="municipality-name">市町村名</label><br><input type="text" name="municipality_name"><br>
        <label for="event-name">イベント名</label><br><input type="text" name="event_name"><br>
        <label for="event-name-kana">イベント名(カナ)</label><br><input type="text" name="event_name_kana"><br>
        <label for="event-name-english">イベント名(英語)</label><br><input type="text" name="event_name_english"><br>
        <label for="category">カテゴリー</label><br>
        <select class="select" name="category">
            <option>イベント</option>
            <option>おしらせ</option>
            <option>おんがく</option>
            <option>講座・教室</option>
            <option>こそだて</option>
            <option>職員募集</option>
            <option>スポーツ</option>
            <option>そうだん</option>
        </select><br>
        <label for="district">地区</label><br><input type="text" name="district"><br>
        <label for="event-details">イベント詳細</label><br><textarea name="event_details"></textarea><br>
        </div>
    </div>
    <button id="next-btn1" class="next-btn btn-main font-weight-bold mt-3" onclick="moveTonextQ(1)">次へ</button>
    </form>

